.col-md-12.padding--top--10px.padding--bottom--10px
    h3.text-info.col-md-offset-4 Test Details               
    .table-responsive(ng-if="rtests.length > 0")
        table.table
            thead.thead-default
                tr
                    td #
                    td Report ID
                    td Patient ID
                    td Time stamp
            tbody(ng-repeat="test in rtests")
                tr
                    td {{$index + 1}}
                    td {{test.ID}}
                    td {{test.patientID}}
                    td {{test.time}}                                     
                    td.no_padding
                        a.btn.btn-link(href="#", ng-click="getTest(test._id)")
                            i.fa.fa-plus
                tr(ng-show="test._id===dtest._id",ng-if="test.bloodTest.length > 0")
                    td(colspan="6")
                        .col-md-12.col-sm-12.col-xs-12
                            table.col-md-12
                                th
                                    td.text-warning Report
                                    td.col-md-4.padding--right--50px.text-align--right
                                        a.btn.btn-link(href="#", ng-click="dtest._id='null'")
                                            i.fa.fa-minus
                                tbody(ng-repeat="t in test.bloodTest in test")
                                    tr
                                        table.col.md-12
                                            tr
                                                td.col-md-3.text-info Patient ID
                                                td.col-md-3.text-success {{test.patientID}}                                  
                                            tr(ng-if="t.collected!=null")   
                                                td.col-md-3.text-info Collected Date
                                                td.col-md-3.text-success {{test.collected}} 
                                            tr(ng-if="t.tested!=null")
                                                td.col-md-3.text-info Tested Date
                                                td.col-md-3.text-success {{t.tested}} 
                                            tr(ng-if="t.volume!=null")
                                                td.col-md-3.text-info Volume
                                                td.col-md-3.text-success {{t.volume}} (ml)
                                            tr(ng-if="t.redBloodCells!=null")
                                                td.col-md-3.text-info RBC 
                                                td.col-md-3.text-success {{t.redBloodCells}} (cells/mcl) 
                                            tr(ng-if="t.whiteBloodCells!=null")
                                                td.col-md-3.text-info WBC 
                                                td.col-md-3.text-success {{t.whiteBloodCells}} (cells/mcl)
                                            tr(ng-if="t.platelets!=null")
                                                td.col-md-3.text-info Platelets
                                                td.col-md-3.text-success {{t.platelets}} (cells/mcl)
                                            tr(ng-if="t.hemoglobin!=null")
                                                td.col-md-3.text-info Hemoglobin 
                                                td.col-md-3.text-success {{t.hemoglobin}} (gm/dl)
                                            tr(ng-if="t.hematocrit!=null")
                                                td.col-md-3.text-info Hematocrit 
                                                td.col-md-3.text-success {{t.hematocrit}} (%)
                                            tr(ng-if="t.plasmaGlugoseLevel!=null")
                                                td.col-md-3.text-info Plasma Glugose Level
                                                td.col-md-3.text-success {{t.plasmaGlugoseLevel}}
                                            tr(ng-if="t.totalCholesterolLevel!=null")
                                                td.col-md-3.text-info Total Cholestrol Level
                                                td.col-md-3.text-success {{t.totalCholesterolLevel}}
                                            tr(ng-if="t.lDLCholesterolLevel!=null")
                                                td.col-md-3.text-info LDL Cholesterol Level
                                                td.col-md-3.text-success {{t.lDLCholesterolLevel}}
                                            tr(ng-if="t.hDLCholesterolLevel!=null")
                                                td.col-md-3.text-info HDL Cholesterol Level
                                                td.col-md-3.text-success {{t.hDLCholesterolLevel}}
                                            tr
                                                td.col-md-3.text-info Generated Time
                                                td.col-md-3.text-success {{test.time}}



